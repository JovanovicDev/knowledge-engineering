FUNCTION_BLOCK fuzzy_validity

VAR_INPUT
	cpuCores: REAL;
	cpuThreads: REAL;
	cpuFrequency: REAL;
	gpuMemory: REAL;
	ramCapacity: REAL;
	ramFrequency: REAL;
	ssdCapacity: REAL;
	ssdWriteSpeed: REAL;
	ssdReadSpeed: REAL;
	psuPower: REAL;
END_VAR

VAR_OUTPUT
	homeUse: REAL;
	professionalUse: REAL;
	appDevelopment: REAL;
	hosting: REAL;
	gaming: REAL;
	cryptoMining: REAL;
END_VAR

FUZZIFY cpuCores
	TERM low := (0, 1) (2, 1) (4, 0);
    TERM medium := (2, 0) (4, 1) (6, 0);
    TERM high := (4, 0) (6, 1) (8, 1);
    TERM ultra := (8, 0) (12, 1) (16, 1);
END_FUZZIFY

FUZZIFY cpuThreads
	TERM low := (0, 1) (4, 1) (8, 0);
    TERM medium := (4, 0) (8, 1) (12, 0);
    TERM high := (8, 0) (12, 1) (16, 0);
    TERM ultra := (16, 0) (24, 1) (32, 1);
END_FUZZIFY

FUZZIFY cpuFrequency
	TERM low := (0, 1) (2, 1) (3, 0);
    TERM medium := (2, 0) (3, 1) (3.4, 0);
    TERM high := (3.4, 0) (3.8, 1) (4, 0);
    TERM ultra := (4, 0) (4.4, 1) (5, 1);
END_FUZZIFY

FUZZIFY gpuMemory
	TERM low := (0, 1) (4, 1) (8, 0);
    TERM medium := (4, 0) (8, 1) (16, 0);
    TERM high := (8, 0) (16, 1) (20, 0);
    TERM ultra := (16, 0) (20, 1) (24, 1);
END_FUZZIFY

FUZZIFY ramCapacity
	TERM low := (0, 1) (4, 1) (8, 0);
    TERM medium := (4, 0) (8, 1) (16, 0);
    TERM high := (8, 0) (16, 1) (32, 0);
    TERM ultra := (16, 0) (32, 1) (64, 1);
END_FUZZIFY

FUZZIFY ramFrequency 
	TERM low := (0, 1) (1600, 1) (2400, 0);
    TERM medium := (1600, 0) (2400, 1) (3600, 0);
    TERM high := (2400, 0) (3600, 1) (5600, 0);
    TERM ultra := (3600, 0) (5600, 1) (7200, 1);
END_FUZZIFY

FUZZIFY ssdCapacity
	TERM low := (0, 1) (256, 1) (512, 0);
    TERM medium := (256, 0) (512, 1) (1000, 0);
    TERM high := (512, 0) (1000, 1) (2000, 0);
    TERM ultra := (1000, 0) (2000, 1) (4000, 1);
END_FUZZIFY

FUZZIFY ssdWriteSpeed
	TERM low := (0, 1) (300, 1) (600, 0);
    TERM medium := (400, 0) (600, 1) (4000, 0);
    TERM high := (600, 0) (4000, 1) (5600, 0);
    TERM ultra := (4000, 0) (5600, 1) (7000, 1);
END_FUZZIFY

FUZZIFY ssdReadSpeed
	TERM low := (0, 1) (300, 1) (600, 0);
    TERM medium := (400, 0) (600, 1) (3500, 0);
    TERM high := (600, 0) (3500, 1) (6000, 0);
    TERM ultra := (3500, 0) (6000, 1) (9000, 1);
END_FUZZIFY

FUZZIFY psuPower
	TERM low := (0, 1) (200, 1) (300, 0);
    TERM medium := (200, 0) (300, 1) (600, 0);
    TERM high := (300, 0) (600, 1) (850, 0);
    TERM ultra := (600, 0) (850, 1) (1200, 1);
END_FUZZIFY

DEFUZZIFY homeUse
    TERM bad := (0, 0) (30, 1) (50, 0);
    TERM mediocre := (30, 0) (50, 1) (75, 0);
    TERM good := (50, 0) (75, 1) (100, 1);
    METHOD : COG;
	DEFAULT := 0;
END_DEFUZZIFY

DEFUZZIFY professionalUse
    TERM bad := (0, 0) (30, 1) (50, 0);
