    TERM good := (50, 0) (75, 1) (100, 1);
    METHOD : COG;
	DEFAULT := 0;
END_DEFUZZIFY

DEFUZZIFY hosting
    TERM bad := (0, 0) (30, 1) (50, 0);
    TERM mediocre := (30, 0) (50, 1) (75, 0);
    TERM good := (50, 0) (75, 1) (100, 1);
    METHOD : COG;
	DEFAULT := 0;
END_DEFUZZIFY

DEFUZZIFY gaming
